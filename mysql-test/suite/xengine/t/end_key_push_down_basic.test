--disable_warnings
drop table if exists t1;
drop table if exists t2;
--enable_warnings

# no hidden pk
create table t1 (
  k1 int,
  k2 int,
  k3 int,
  v1 int,
  v2 int,
  v3 int,
  primary key (k1, k2),
  unique key ui1 (k2, k3),
  key i1 (v1, v2, v3)
) engine=xengine;

# hidden pk
create table t2 (
  k1 int,
  k2 int,
  k3 int,
  v1 int,
  v2 int,
  v3 int,
  key i2 (v1, v2, v3)
) engine=xengine;

